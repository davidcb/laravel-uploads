{
	"name": "davidcb/laravel-uploads",
	"description": "Uploads management for Laravel",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "David Couto",
			"email": "davidcouto@gmail.com"
		}
	],
	"require": {
		"php": ">=7",
        "illuminate/support": "5.5.x|5.6.x|5.7.x",
		"spatie/eloquent-sortable": "3.*",
		"intervention/image": "2.*"
	},
	"autoload": {
		"psr-4": {
			"Davidcb\\Uploads\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Davidcb\\Uploads\\UploadsServiceProvider"
			],
			"aliases": {
				"LaravelUploads": "Davidcb\\Uploads\\Facade"
			}
		}
	},
	"config": {
        "preferred-install": "source"
    }
}
